{
    "version": "2.0",
    "extensions": {
        "Sql": {
          "MaxBatchSize": 300,
          "PollingIntervalMs": 1000,
          "MaxChangesPerWorker": 100
        }
    },
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        }
      },
      "logLevel": {
        "default": "Trace"
      }
    }
  }